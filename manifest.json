{   
    "manifest_version": 2,
    "name": "Scroll Buttons",
    "description": "Extension with buttons that scroll through the page",  
    "version": "1.0",
    "icons": {
        "16": "/images/16.png",
        "48": "/images/48.png",
        "128": "/images/128.png"
    },
    "page_action": {
        "default_icon": {
            "16": "/images/16.png",
            "48": "/images/48.png",
            "128": "/images/128.png"
        }
    },
    "content_scripts": [{
        "matches": ["https://*.com/*"],
        "js": ["app.js"]
    }]
  }  
  